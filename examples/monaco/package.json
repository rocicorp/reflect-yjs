{
  "name": "reflect-yjs-monaco",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --write *",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "watch": "concurrently --kill-others 'npx reflect dev --silence-startup-message' 'VITE_REFLECT_URL=http://127.0.0.1:8080/ npm run dev'"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "@rocicorp/reflect": "^0.39.202402020122",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "badwords-list": "^1.0.0",
    "concurrently": "^8.2.2",
    "monaco-editor": "^0.44.0",
    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-yjs": "0.0.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "y-monaco": "^0.1.5",
    "yjs": "^13.6.10"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
